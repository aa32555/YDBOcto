
-- TWI03 : where IN references grandparent query

SELECT *
FROM names a
WHERE a.firstName IN (
    SELECT b.firstName
    FROM names b
    WHERE b.id IN (SELECT c.id
        FROM names c
        WHERE c.id = a.id)
);

