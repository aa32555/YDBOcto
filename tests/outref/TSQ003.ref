OCTO>  SELECT NULL          AS TABLE_CAT,
OCTO>        n.nspname     AS TABLE_SCHEM,
OCTO>        c.relname     AS TABLE_NAME,
OCTO>        CASE n.nspname ~ '^pg_'
OCTO>              OR n.nspname = 'information_schema'
OCTO>          WHEN true THEN
OCTO>            CASE
OCTO>              WHEN n.nspname = 'pg_catalog'
OCTO>                    OR n.nspname = 'information_schema' THEN
OCTO>                CASE c.relkind
OCTO>                  WHEN 'r' THEN 'SYSTEM TABLE'
OCTO>                  WHEN 'v' THEN 'SYSTEM VIEW'
OCTO>                  WHEN 'i' THEN 'SYSTEM INDEX'
OCTO>                  ELSE NULL
OCTO>                end
OCTO>              WHEN n.nspname = 'pg_toast' THEN
OCTO>                CASE c.relkind
OCTO>                  WHEN 'r' THEN 'SYSTEM TOAST TABLE'
OCTO>                  WHEN 'i' THEN 'SYSTEM TOAST INDEX'
OCTO>                  ELSE NULL
OCTO>                end
OCTO>              ELSE
OCTO>                CASE c.relkind
OCTO>                  WHEN 'r' THEN 'TEMPORARY TABLE'
OCTO>                  WHEN 'p' THEN 'TEMPORARY TABLE'
OCTO>                  WHEN 'i' THEN 'TEMPORARY INDEX'
OCTO>                  WHEN 'S' THEN 'TEMPORARY SEQUENCE'
OCTO>                  WHEN 'v' THEN 'TEMPORARY VIEW'
OCTO>                  ELSE NULL
OCTO>                end
OCTO>            end
OCTO>          WHEN false THEN
OCTO>            CASE c.relkind
OCTO>              WHEN 'r' THEN 'TABLE'
OCTO>              WHEN 'p' THEN 'TABLE'
OCTO>              WHEN 'i' THEN 'INDEX'
OCTO>              WHEN 'S' THEN 'SEQUENCE'
OCTO>              WHEN 'v' THEN 'VIEW'
OCTO>              WHEN 'c' THEN 'TYPE'
OCTO>              WHEN 'f' THEN 'FOREIGN TABLE'
OCTO>              WHEN 'm' THEN 'MATERIALIZED VIEW'
OCTO>              ELSE NULL
OCTO>            end
OCTO>          ELSE NULL
OCTO>        end           AS TABLE_TYPE,
OCTO>        d.description AS REMARKS,
OCTO>        ''            AS TYPE_CAT,
OCTO>        ''            AS TYPE_SCHEM,
OCTO>        ''            AS TYPE_NAME,
OCTO>        ''            AS SELF_REFERENCING_COL_NAME,
OCTO>        ''            AS REF_GENERATION
OCTO> FROM   pg_catalog.pg_namespace n,
OCTO>        pg_catalog.pg_class c
OCTO>        LEFT JOIN pg_catalog.pg_description d
OCTO>               ON ( c.oid = d.objoid
OCTO>                    AND d.objsubid = 0 )
OCTO>        LEFT JOIN pg_catalog.pg_class dc
OCTO>               ON ( d.classoid = dc.oid
OCTO>                    AND dc.relname = 'pg_class' )
OCTO>        LEFT JOIN pg_catalog.pg_namespace dn
OCTO>               ON ( dn.oid = dc.relnamespace
OCTO>                    AND dn.nspname = 'pg_catalog' )
OCTO> WHERE  c.relnamespace = n.oid
OCTO>        AND c.relname LIKE '%'
OCTO>        AND ( false
OCTO>               OR ( c.relkind IN ( 'r', 'p' )
OCTO>                    AND n.nspname !~ '^pg_'
OCTO>                    AND n.nspname <> 'information_schema' )
OCTO>               OR ( c.relkind = 'r'
OCTO>                    AND ( n.nspname = 'pg_catalog'
OCTO>                           OR n.nspname = 'information_schema' ) )
OCTO>               OR ( c.relkind = 'v'
OCTO>                    AND n.nspname <> 'pg_catalog'
OCTO>                    AND n.nspname <> 'information_schema' ) )
OCTO> ORDER  BY table_type,
OCTO>           table_schem,
OCTO>           table_name
