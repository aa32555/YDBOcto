load test_helpers

setup() {
  init_test
  createdb
  load_fixture pastas.sql
  load_fixture pastas.zwr
}

@test "TIJOB01 : inner join pastas order by pasta id ascending" {
  octo <<OCTO &> output.txt
select pastas.id, firstName, lastName, pastaName from names4
inner join pastas on pastas.pastaName = names4.favoritePasta
order by pastas.id;
OCTO
  verify_output TIJOB01 output.txt noinfo nodebug
}

@test "TIJOB02 : inner join pastas order by pasta id ascending explicit" {
  octo <<OCTO &> output.txt
select pastas.id, firstName, lastName, pastaName from names4
inner join pastas on pastas.pastaName = names4.favoritePasta
order by pastas.id asc;
OCTO
  verify_output TIJOB02 output.txt noinfo nodebug
}

